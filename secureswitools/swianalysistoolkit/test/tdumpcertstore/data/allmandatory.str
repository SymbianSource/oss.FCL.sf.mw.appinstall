;This create the certstore for testing writeable certstore.

[SymbianTestRootCADiffSerial]
file = c:\tswi\certstore\SymbianTestRootCADiffSerial.der
capability = ReadDeviceData
capability = WriteDeviceData
capability = DRM
capability = AllFiles
application = SWInstall
Mandatory=1


[Root5CA]
file = c:\tswi\certstore\Root5CA.der
application = SWInstall
Mandatory=1

[SymbianTestRSACA]
file = c:\tswi\certstore\Symbian-Test-RSA.der
capability = TCB
capability = CommDD
application = SWInstall
Mandatory=1


