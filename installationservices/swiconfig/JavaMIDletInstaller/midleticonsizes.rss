#include <midleticonsizes.rh>

RESOURCE MIDLET_ICON_SIZES r_icon_sizes
	{
	aspect = EPreserveAspect;
	scaling = EScaleDown;
	
	sizes =
		{
		MIDLET_ICON_SIZE
			{
			width=12;
			height=12;
			},
		MIDLET_ICON_SIZE
			{
			width = 24;
			height = 24;
			},
		MIDLET_ICON_SIZE
			{
			width = 32;
			height = 32;
			}
		};

	}
